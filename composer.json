{
  "name": "itsnubix/nowcal",
  "version": "1.2.2",
  "description": "A modern PHP library for generating iCalendar v2.0 events",
  "keywords": [
    "icalendar",
    "calendar",
    "ics"
  ],
  "support": {
    "issues": "https://github.com/itsnubix/nowcal/issues",
    "source": "https://github.com/itsnubix/nowcal"
  },
  "require": {
    "php": ">8.1"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.0",
    "phpunit/phpunit": "^7.1.5|^9.5"
  },
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Kyle Milloy",
      "email": "kmilloy@nubix.ca"
    }
  ],
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "NowCal\\": "src/NowCal/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
}
